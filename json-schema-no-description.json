{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "contact": {
            "type": "object",
            "properties": {
                "salutation": {
                    "type": "string",
                    "enum": [
                        "Monsieur",
                        "Madame",
                        "Mademoiselle"
                    ],
                },
                "prenom": {
                    "type": "string",
                },
                "nom": {
                    "type": "string",
                },
                "dateDeNaissance": {
                    "type": "string",
                    "format": "date",
                },
                "statutMarital": {
                    "type": "string",
                    "enum": [
                        "Célibataire",
                        "Marrié(e)",
                        "Divorcé(e)",
                        "Feuf(ve)"
                    ],
                },
                "nombreEnfants": {
                    "type": "integer",
                },
                "enfants": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "prenom": {
                                "type": "string",
                            },
                            "nom": {
                                "type": "string",
                            },
                            "dateDeNaissance": {
                                "type": "string",
                                "format": "date",
                            },
                            "situation": {
                                "type": "string",
                            }
                        }
                    },
                },
                "addresse": {
                    "type": "object",
                    "properties": {
                        "numeroDeRue": {
                            "type": "string",
                        },
                        "rue": {
                            "type": "string",
                        },
                        "complement": {
                            "type": "string",
                        },
                        "codePostal": {
                            "type": "string",
                        },
                        "ville": {
                            "type": "string",
                        },
                        "pays": {
                            "type": "string",
                        },
                        "longitude": {
                            "type": "number",
                        },
                        "latitude": {
                            "type": "number",
                        }
                    },
                },
                "email": {
                    "type": "string",
                },
                "telephoneDomicile": {
                    "type": "string",
                },
                "telephoneMobile": {
                    "type": "string",
                },
                "telephoneBureau": {
                    "type": "string",
                },
                "typeTelephonePrefere": {
                    "type": "string",
                },
                "profession": {
                    "type": "object",
                    "properties": {
                        "libelle": {
                            "type": "string",
                        },
                        "situation": {
                            "type": "string",
                            "enum": [
                                "Salarié",
                                "Indépendant",
                                "Etudiant",
                                "Retraité",
                                "Sans emploi"
                            ],
                        },
                        "categorie": {
                            "type": "string",
                            "enum": [
                                "Cadre",
                                "Employé",
                                "Ouvrier",
                                "Agriculteur",
                                "Artisan",
                                "Commerçant",
                                "Profession libérale",
                                "Fonctionnaire",
                                "Retraité",
                                "Autre"
                            ],
                        }
                    },
                }
            },
        },
        "professionnel": {
            "type": "object",
            "properties": {
                "identifiant": {
                    "type": "string",
                },
                "email": {
                    "type": "string",
                },
                "nom": {
                    "type": "string",
                }
            }
        },
        "projet": {
            "type": "object",
            "properties": {
                "identifiant": {
                    "type": "string",
                },
                "typeProjet": {
                    "type": "string",
                    "enum": [
                        "Achat",
                        "Vente",
                        "Bail",
                        "Location"
                    ],
                },
                "dateCreation": {
                    "type": "string",
                    "format": "date-time",
                },
                "dateDebut": {
                    "type": "string",
                    "format": "date-time",
                },
                "statut": {
                    "type": "string",
                    "enum": [
                        "En Réflexion",
                        "Actif",
                        "Suspendu",
                        "Terminé"
                    ],
                },
               
                "detailsAchat": {
                    "type": "object",
                    "properties": {
                        "typeBien": {
                            "type": "string",
                            "enum": [
                                "Maison",
                                "Appartement",
                                "Terrain",
                                "Parking",
                                "Garage",
                                "Local commercial",
                                "Autre"
                            ],
                        },
                        "zoneRecherche": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "numeroDeRue": {
                                        "type": "string",
                                    },
                                    "rue": {
                                        "type": "string",
                                    },
                                    "complement": {
                                        "type": "string",
                                    },
                                    "codePostal": {
                                        "type": "string",
                                    },
                                    "ville": {
                                        "type": "string",
                                    },
                                    "pays": {
                                        "type": "string",
                                    },
                                    "longitude": {
                                        "type": "number",
                                    },
                                    "latitude": {
                                        "type": "number",
                                    },
                                    "rayon": {
                                        "type": "number",
                                    }
                                },
                            }
                        },
                        "criteresBien": {
                            "type": "object",
                            "properties": {
                                "pieces": {
                                    "type": "integer",
                                },
                                "surface": {
                                    "type": "number",
                                },
                                "surfaceTerrain": {
                                    "type": "number",
                                },
                                "etage": {
                                    "type": "integer",
                                },
                                "nombreEtages": {
                                    "type": "integer",
                                },
                                "sallesDeBain": {
                                    "type": "integer",
                                },
                                "chambres": {
                                    "type": "integer",
                                },
                                "placesGarage": {
                                    "type": "integer",
                                },
                                "placesParking": {
                                    "type": "integer",
                                },
                                "jardin": {
                                    "type": "boolean",
                                },
                                "balcon": {
                                    "type": "boolean",
                                },
                                "condition": {
                                    "type": "string",
                                    "enum": [
                                        "Neuf",
                                        "A rénover",
                                        "Bon état"
                                    ],
                                },
                                "orientation": {
                                    "type": "string",
                                    "enum": [
                                        "Nord",
                                        "Sud",
                                        "Est",
                                        "Ouest"
                                    ],
                                }
                            },
                        },
                        "informationsFinancieres": {
                            "type": "object",
                            "properties": {
                                "montantDuBien": {
                                    "type": "number",
                                },
                                "montantTravaux": {
                                    "type": "number",
                                },
                                "montantProjet": {
                                    "type": "number",
                                },
                                "montantApport": {
                                    "type": "number",
                                },
                                "financementValide": {
                                    "type": "boolean",
                                }
                            },
                        },
                        "proximites": {
                            "type": "object",
                            "properties": {
                                "ecole": {
                                    "type": "boolean",
                                },
                                "creche": {
                                    "type": "boolean",
                                },
                                "hopital": {
                                    "type": "boolean",
                                },
                                "commerces": {
                                    "type": "boolean",
                                }
                            },
                        }
                    },
                },
                "detailsVente": {
                    "type": "object",
                    "properties": {
                        "typeBien": {
                            "type": "string",
                            "enum": [
                                "Maison",
                                "Appartement",
                                "Terrain",
                                "Parking",
                                "Garage",
                                "Local commercial",
                                "Autre"
                            ],
                        },
                        "pieces": {
                            "type": "integer",
                        },
                        "surface": {
                            "type": "number",
                        },
                        "surfaceTerrain": {
                            "type": "number",
                        },
                        "etage": {
                            "type": "integer",
                        },
                        "nombreEtages": {
                            "type": "integer",
                        },
                        "condition": {
                            "type": "string",
                            "enum": [
                                "Neuf",
                                "À rénover",
                                "Bon état"
                            ],
                        },
                        "orientation": {
                            "type": "string",
                            "enum": [
                                "Nord",
                                "Sud",
                                "Est",
                                "Ouest"
                            ],
                        },
                        "adresse": {
                            "type": "object",
                            "properties": {
                                "numeroDeRue": {
                                    "type": "string",
                                },
                                "rue": {
                                    "type": "string",
                                },
                                "complement": {
                                    "type": "string",
                                },
                                "codePostal": {
                                    "type": "string",
                                },
                                "ville": {
                                    "type": "string",
                                },
                                "pays": {
                                    "type": "string",
                                },
                                "longitude": {
                                    "type": "number",
                                },
                                "latitude": {
                                    "type": "number",
                                }
                            },
                        },
                        "informationsFinancieres": {
                            "type": "object",
                            "properties": {
                                "prixAttendu": {
                                    "type": "number",
                                },
                                "montantEstimeRenovation": {
                                    "type": "number",
                                },
                                "prixEstime": {
                                    "type": "number",
                                },
                                "nomProfessionnelEstimation": {
                                    "type": "number",
                                }
                            },
                        },
                        "estimation": {
                            "type": "object",
                            "properties": {
                                "prixEstime": {
                                    "type": "number",
                                },
                                "nomProfessionnel": {
                                    "type": "string",
                                },
                                "date": {
                                    "type": "string",
                                    "format": "date",
                                }
                            },
                        },
                        "proximites": {
                            "type": "object",
                            "properties": {
                                "ecole": {
                                    "type": "boolean",
                                },
                                "creche": {
                                    "type": "boolean",
                                },
                                "hopital": {
                                    "type": "boolean",
                                },
                                "commerces": {
                                    "type": "boolean",
                                }
                            },
                        }
                    },
                }
            },
        }
    },
    "required": [
        "applicant",
        "project"
    ],
}
